$tangerine:#f29b29;
$pink:#ea1c7c;
$purple:#b22790;
$chartruese:#dee04a;
$chartruese_trans:rgba(224,224,71, 0.75);
$lime:#aac437;
html,body{
  font-family: 'Love Ya Like A Sister', cursive;
  // background-image: image-url("<%= asset_path('img-noise-tangering361x370') %>");
  background-repeat: repeat;
 }
 body.tacos-index{
  color:#FFF;
 }
 .modal p, .modal ul li{
  color:#333;
 }
 header{
  margin-bottom:5%;
 }
 .navbar-default{
  background-color: rgba(0, 0, 0, 0.2);
  border:none;
  z-index: 54;
 }
 .navbar-default .navbar-nav > li > a {
  color: $pink;
  font-size: 200%;
}
.navbar-nav > li > .dropdown-menu{
  z-index: 99;
}
.navbar-default .navbar-collapse, .navbar-default .navbar-form{
  border:none;
}
.navbar-default .navbar-nav > .open > a, .navbar-default .navbar-nav > .open > a:hover, .navbar-default .navbar-nav > .open > a:focus{
  background-color:transparent;
}
.navbar-default .navbar-nav > li > a:hover, .navbar-default .navbar-nav > li > a:focus{
  color:$chartruese;
}
.dropdown-toggle{
  position:absolute;
  top:10px;
  right:10px;
  float:right;
}
 .container-fluid{
  padding:0;
 }
 body.tacos-show,body.tacos-new,body.sessions-new{
  background-image: image-url("<%= asset_path('wood_tile.jpg') %>");
  background-repeat: repeat;
  background-size:contain;
 }
 .tacos-show .container-fluid,.tacos-new .container-fluid,.sessions-new .container-fluid{
  padding:0 5%;
 }
 .tacos-new .taco_single,.sessions-new .taco_single{
  margin-top:100px;
  background-color:$chartruese;
  overflow:auto;
 }
 .tacos-new .taco_single input,.sessions-new .taco_single input{
  color:$pink;
 }
  .tacos-new .taco_single input[type="submit"],.sessions-new .taco_single input[type="submit"]{
  color:#FFF;
 }
 .tacos-new .taco_single label,.sessions-new .taco_single label{
  color:$purple;
 }
 .signup-in{
  z-index: 18;
 }
 h1{
  font-family: 'Love Ya Like A Sister', cursive;
  font-size: 12vw;
  color:#FFF;
 }
 @media all and (min-width: 768px){
   h1{
    font-size: 6vw;
   }
}
header h1{
  text-align: center;
}
 h2{
  font-family: 'Love Ya Like A Sister', cursive;
  font-size: 7vw;
  line-height: 1.25;
  color:$pink;
 }
 @media all and (min-width: 768px){
   h2{
    font-size: 2.5vw;
   }
}
h2 a{
  cursor:pointer;
}
h3.restaurant{
  color:$purple;
}
h3.taco_description{
  color:#FFF;
}
h4{
  font-family: 'Love Ya Like A Sister', cursive;
  color:#b22790;
}
.tacos-show h4{
  color:#333;
}
 .rating{
  color:#73152D;
 }
 a{
  color:#1A1830;
 }
 label{
  display: block;
 }
 .tacos-show label{
  display: inline-block;
  margin-right:10px;
 }
 input, textarea, option{
  color:#73152D;
  font-family: 'Love Ya Like A Sister', cursive;
  padding: 10px;
  font-size: 1.5em;
 }
 .taco_single{
  background: #8B5F49;
  background:rgba(139,95,73,0.5);
  padding:0 15px 15px 15px;
  color:#FFF;
  box-shadow: 0 3px 3px rgba(0,0,0,0.35);
  min-height: 550px;
  z-index: 9999;
 }
 .taco_single_image{
  margin-bottom: 20px;
  width:100%;
  border: 4px solid #FFF;
  box-shadow: 0px 1px 2px rgba(0,0,0,0.25);
 }
 .btn{
  margin-top:20px;
  border-radius:0;
 }
 .btn-warning {
    color: #fff;
    background-color: $pink;
    border:none;
  }
  .btn-warning:hover{
    background-color:$lime;
  }
  .btn-break{
    display:block;
  }
 /* UI============UI */
 /* Index */
.top-tacos-list{
  height: 60%;
  overflow: auto;
  z-index: 18;
  padding:10px;
  position: relative;
  right:0;
  background-color:$chartruese;
  color: #FFF;
  text-align: left;
    box-shadow: 0px 1px 2px rgba(0,0,0,0.25);
}
p.map_it a{
  color:$purple;
  font-size: 150%;
}
.top-tacos-list h2{
  color: $pink;
}
.top-tacos-list h3{
  margin-top:0;
  font-size:1em;
  color:#333;
}
.top-tacos-list h4{
  margin-top: 0;
}
.top-tacos-list a {
  color:$purple;
}
@media all and (min-width: 768px){
 .top-tacos-list{
  position: fixed;
  right:20px;
  }
}
.single-taco-stats{
  overflow: auto;
  z-index: 27;
  padding:10px;
  position: relative;
  right:auto;
  background-color:$chartruese_trans;
  text-align: left;
  box-shadow: 0px 1px 2px rgba(0,0,0,0.25);
}
@media all and (min-width: 768px){
  .single-taco-stats{
    position: absolute;
    right:100px;
  }
}
@media all and (min-width: 768px){
  #taco-map {
    position:absolute;
    top:0;
    right:0;
    bottom:0;
    left:0;
    width: 100%;
    margin: 0;
    padding: 0;
    z-index: 17;
  }
}
.sessions-new .taco-map-container, .tacos-new .taco-map-container, .tacos-show .taco-map-container, .tacos-edit .taco-map-container, .restaurants-index .taco-map-container{
  display: none;
}
#taco_why,#taco_stage1,#taco_stage2,#taco_stage3,#taco_stage4,#taco_stage5{
  height: 100vh;
  overflow: hidden;
  // text-align: center;
  position: relative;
}
#taco_main h1,#taco_why h1,#taco_stage1 h1,#taco_stage2 h1,#taco_stage3 h1,#taco_stage4 h1,#taco_stage5 h1,#taco_stage5 h2{
  text-align: center;
}
#taco_main{
  height: 80vh;
  overflow: hidden;
  text-align: center;
  position: relative;
}
#taco_why{
  // background-color:$tangerine;
  background-image: image-url("<%= asset_path('why_taquistador.jpg') %>");
  background-size:100%;
  background-repeat:no-repeat;
}
.why_rows{
  font-size: 1.25em;
}
#taco_stage1{
  background-color:$purple;
}
#taco_stage1 h2{
  color:$chartruese;
}
#taco_stage2{
  background-color:$pink;
}
#taco_stage2 h2{
  color:$chartruese;
  text-decoration: line-through;
}
#taco_stage3{
  background-color:$lime;
}
#taco_stage4{
  background-color:$chartruese;
}
#taco_stage4 p{
  color:#333;
}
#taco_stage5{
  // background-color:$tangerine;
  background-image: image-url("<%= asset_path('salud.jpg') %>");
  background-size:100%;
  background-repeat:no-repeat;
}
#taco_stage2{
  background-image: url(<%= image_url('corn-tortilla.png') %> );
  background-size: 125%;
  background-position: 50% 50%;
  background-repeat: no-repeat;
}
@media all and (min-width: 768px){
  #taco_stage2{
    background-size: 40%;
  }
}
#taco_stage3{
  background-image: url(<%= image_url('corn-tortilla-carnitas.png') %> );
  background-size: 125%;
  background-position: 50% 50%;
  background-repeat: no-repeat;
}
@media all and (min-width: 768px){
  #taco_stage3{
    background-size: 40%;
  }
}
#taco_stage4{
  background-image: url(<%= image_url('corn-tortilla-carnitas-onion-cilantro.png') %> );
  background-size: 125%;
  background-position: 50% 50%;
  background-repeat: no-repeat;
}
@media all and (min-width: 768px){
  #taco_stage4{
    background-size: 40%;
  }
}
