html,body{
  font-family: 'Love Ya Like A Sister', cursive;
  background-image: image-url("<%= asset_path('img-noise-361x370.png') %>");
  background-repeat: repeat;
 }
 header{
  margin-bottom:5%;
 }
 .navbar-default{
  background-color: rgba(0, 0, 0, 0.2);
  border:none;
  z-index: 54;
 }
 .navbar-default .navbar-nav > li > a {
  color: #73152D;
  font-size: 200%;
}
.navbar-nav > li > .dropdown-menu{
  z-index: 99;
}
 .container-fluid{
  padding:0 5%;
 }
 .signup-in{
  z-index: 18;
 }
 h1{
  font-family: 'Love Ya Like A Sister', cursive;
  font-size: 12vw;
  color:#73152D;
 }
 @media all and (min-width: 768px){
   h1{
    font-size: 6vw;
   }
}
header h1{
  text-align: center;
}
 h2{
  font-family: 'Love Ya Like A Sister', cursive;
  font-size: 7vw;
  line-height: 1.25;
  color:#73152D;
 }
 @media all and (min-width: 768px){
   h2{
    font-size: 2.5vw;
   }
}
 .rating{
  color:#73152D;
 }
 a{
  color:#1A1830;
 }
 label{
  display: block;
 }
 input, textarea, option{
  color:#73152D;
  font-family: 'Love Ya Like A Sister', cursive;
  padding: 10px;
  font-size: 1.5em;
 }
 .taco_single{
  background: #8B5F49;
  background:rgba(139,95,73,0.5);
  padding:0 15px 15px 15px;
  color:#FFF;
  box-shadow: 0 3px 3px rgba(0,0,0,0.35);
  min-height: 550px;
  z-index: 9999;
 }
 .taco_single_image{
  margin-bottom: 20px;
  width:100%;
 }
 .btn{
  margin-top:20px;
 }
 /* UI============UI */
 /* Index */
.top-tacos-list{
  height: 60%;
  overflow: auto;
  z-index: 18;
  padding:10px;
  position: relative;
  right:0;
  background-color:#73152D;
  color: #FFF;
  text-align: left;
}
.top-tacos-list h2{
  color: #FFF;
}
.top-tacos-list h3{
  margin-top:0;
  font-size:1.5em;
}
.top-tacos-list h4{
  margin-top: 0;
}
@media all and (min-width: 768px){
 .top-tacos-list{
  position: fixed;
  right:20px;
  }
}
@media all and (min-width: 768px){
  #taco-map {
    position:absolute;
    top:0;
    right:0;
    bottom:0;
    left:0;
    width: 100%;
    margin: 0;
    padding: 0;
    z-index: 17;
  }
}
.tacos-new .taco-map-container, .tacos-show .taco-map-container{
  display: none;
}
#taco_main,#taco_why,#taco_stage1,#taco_stage2,#taco_stage3,#taco_stage4,#taco_stage5{
  height: 100vh;
  overflow: hidden;
  text-align: center;
  position: relative;
}
#taco_stage2{
  background-image: url(<%= image_url('corn-tortilla.png') %> );
  background-size: 125%;
  background-position: 50% 50%;
  background-repeat: no-repeat;
}
@media all and (min-width: 768px){
  #taco_stage2{
    background-size: 45%;
  }
}
#taco_stage3{
  background-image: url(<%= image_url('corn-tortilla-carnitas.png') %> );
  background-size: 125%;
  background-position: 50% 50%;
  background-repeat: no-repeat;
}
@media all and (min-width: 768px){
  #taco_stage3{
    background-size: 45%;
  }
}
#taco_stage4{
  background-image: url(<%= image_url('corn-tortilla-carnitas-onion-cilantro.png') %> );
  background-size: 125%;
  background-position: 50% 50%;
  background-repeat: no-repeat;
}
@media all and (min-width: 768px){
  #taco_stage4{
    background-size: 45%;
  }
}
#taco_stage5{
  background-image: url(<%= image_url('taco-all.png') %> );
  background-size: 125%;
  background-position: 50% 50%;
  background-repeat: no-repeat;
}
@media all and (min-width: 768px){
  #taco_stage5{
    background-size: 45%;
  }
}
#tortilla{
  visibility: hidden;
}
#meat{
  z-index: 3;
  visibility: hidden;
}
#garnish{
  z-index: 4;
  visibility: hidden;
}
#salsas{
  z-index: 5;
  visibility: hidden;
}
#taco{
  visibility: hidden;
}
